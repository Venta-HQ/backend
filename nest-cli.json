{
	"$schema": "https://json.schemastore.org/nest-cli",
	"collection": "@nestjs/schematics",
	"sourceRoot": "apps/infrastructure/api-gateway/src",
	"compilerOptions": {
		"deleteOutDir": true,
		"webpack": true,
		"tsConfigPath": "apps/infrastructure/api-gateway/tsconfig.app.json"
	},
	"monorepo": true,
	"root": "apps/infrastructure/api-gateway",
	"projects": {
		"user-management": {
			"type": "application",
			"root": "apps/marketplace/user-management",
			"entryFile": "src/main",
			"sourceRoot": "./",
			"compilerOptions": {
				"assets": ["libs/proto/src/definitions/domains/user/user.proto"],
				"tsConfigPath": "apps/marketplace/user-management/tsconfig.app.json"
			}
		},
		"vendor-management": {
			"type": "application",
			"root": "apps/marketplace/vendor-management",
			"entryFile": "src/main",
			"sourceRoot": "./",
			"compilerOptions": {
				"assets": ["libs/proto/src/definitions/domains/vendor/vendor.proto"],
				"tsConfigPath": "apps/marketplace/vendor-management/tsconfig.app.json"
			}
		},
		"search-discovery": {
			"type": "application",
			"root": "apps/marketplace/search-discovery",
			"entryFile": "src/main",
			"sourceRoot": "./",
			"compilerOptions": {
				"tsConfigPath": "apps/marketplace/search-discovery/tsconfig.app.json"
			}
		},
		"geolocation": {
			"type": "application",
			"root": "apps/location-services/geolocation",
			"entryFile": "src/main",
			"sourceRoot": "./",
			"compilerOptions": {
				"assets": ["libs/proto/src/definitions/domains/location/location.proto"],
				"tsConfigPath": "apps/location-services/geolocation/tsconfig.app.json"
			}
		},
		"real-time-location": {
			"type": "application",
			"root": "apps/location-services/real-time",
			"entryFile": "src/main",
			"sourceRoot": "./",
			"compilerOptions": {
				"assets": ["libs/proto/src/definitions/domains/location/location.proto"],
				"tsConfigPath": "apps/location-services/real-time/tsconfig.app.json"
			}
		},
		"api-gateway": {
			"type": "application",
			"root": "apps/infrastructure/api-gateway",
			"entryFile": "src/main",
			"sourceRoot": "./",
			"compilerOptions": {
				"assets": [
					"libs/proto/src/definitions/domains/user/user.proto",
					"libs/proto/src/definitions/domains/vendor/vendor.proto"
				],
				"tsConfigPath": "apps/infrastructure/api-gateway/tsconfig.app.json"
			}
		},
		"file-management": {
			"type": "application",
			"root": "apps/infrastructure/file-management",
			"entryFile": "src/main",
			"sourceRoot": "./",
			"compilerOptions": {
				"tsConfigPath": "apps/infrastructure/file-management/tsconfig.app.json"
			}
		},
		"nest/modules": {
			"type": "library",
			"root": "libs/nest/modules/",
			"entryFile": "index",
			"sourceRoot": "libs/nest/modules/",
			"compilerOptions": {
				"tsConfigPath": "libs/nest/modules/tsconfig.lib.json"
			}
		},
		"nest/guards": {
			"type": "library",
			"root": "libs/nest/guards/",
			"entryFile": "index",
			"sourceRoot": "libs/nest/guards/",
			"compilerOptions": {
				"tsConfigPath": "libs/nest/guards/tsconfig.lib.json"
			}
		},
		"nest/pipes": {
			"type": "library",
			"root": "libs/nest/pipes/",
			"entryFile": "index",
			"sourceRoot": "libs/nest/pipes/",
			"compilerOptions": {
				"tsConfigPath": "libs/nest/pipes/tsconfig.lib.json"
			}
		},
		"nest/filters": {
			"type": "library",
			"root": "libs/nest/filters/",
			"entryFile": "index",
			"sourceRoot": "libs/nest/filters/",
			"compilerOptions": {
				"tsConfigPath": "libs/nest/filters/tsconfig.lib.json"
			}
		},
		"nest/errors": {
			"type": "library",
			"root": "libs/nest/errors/",
			"entryFile": "index",
			"sourceRoot": "libs/nest/errors/",
			"compilerOptions": {
				"tsConfigPath": "libs/nest/errors/tsconfig.lib.json"
			}
		},
		"nest/interceptors": {
			"type": "library",
			"root": "libs/nest/interceptors/",
			"entryFile": "index",
			"sourceRoot": "libs/nest/interceptors/",
			"compilerOptions": {
				"tsConfigPath": "libs/nest/interceptors/tsconfig.lib.json"
			}
		},
		"proto": {
			"type": "library",
			"root": "libs/proto",
			"entryFile": "index",
			"sourceRoot": "libs/proto/src",
			"compilerOptions": {
				"tsConfigPath": "libs/proto/tsconfig.lib.json"
			}
		},
		"apitypes": {
			"type": "library",
			"root": "libs/apitypes",
			"entryFile": "index",
			"sourceRoot": "libs/apitypes/src",
			"compilerOptions": {
				"tsConfigPath": "libs/apitypes/tsconfig.lib.json"
			}
		}
	}
}
