{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "apps/gateway/src",
  "compilerOptions": {
    "deleteOutDir": true,
    "webpack": true,
    "tsConfigPath": "apps/gateway/tsconfig.app.json"
  },
  "monorepo": true,
  "root": "apps/gateway",
  "projects": {
    "auth": {
      "type": "application",
      "root": "apps/auth",
      "entryFile": "src/main",
      "sourceRoot": "./",
      "compilerOptions": {
        "assets": [
          "libs/proto/src/definitions/auth.proto"
        ],
        "tsConfigPath": "apps/auth/tsconfig.app.json"
      }
    },
    "gateway": {
      "type": "application",
      "root": "apps/gateway",
      "entryFile": "src/main",
      "sourceRoot": "./",
      "compilerOptions": {
        "assets": [
          "libs/proto/src/definitions/auth.proto"
        ],
        "tsConfigPath": "apps/gateway/tsconfig.app.json"
      }
    },
    "nest/modules": {
      "type": "library",
      "root": "libs/nest/modules/",
      "entryFile": "index",
      "sourceRoot": "libs/nest/modules/",
      "compilerOptions": {
        "tsConfigPath": "libs/nest/modules/tsconfig.lib.json"
      }
    },
    "nest/guards": {
      "type": "library",
      "root": "libs/nest/guards/",
      "entryFile": "index",
      "sourceRoot": "libs/nest/guards/",
      "compilerOptions": {
        "tsConfigPath": "libs/nest/guards/tsconfig.lib.json"
      }
    },
    "nest/pipes": {
      "type": "library",
      "root": "libs/nest/pipes/",
      "entryFile": "index",
      "sourceRoot": "libs/nest/pipes/",
      "compilerOptions": {
        "tsConfigPath": "libs/nest/pipes/tsconfig.lib.json"
      }
    },
    "nest/filters": {
      "type": "library",
      "root": "libs/nest/filters/",
      "entryFile": "index",
      "sourceRoot": "libs/nest/filters/",
      "compilerOptions": {
        "tsConfigPath": "libs/nest/filters/tsconfig.lib.json"
      }
    },
    "proto": {
      "type": "library",
      "root": "libs/proto",
      "entryFile": "index",
      "sourceRoot": "libs/proto/src",
      "compilerOptions": {
        "tsConfigPath": "libs/proto/tsconfig.lib.json"
      }
    },
    "location": {
      "type": "application",
      "root": "apps/location",
      "entryFile": "src/main",
      "sourceRoot": "./",
      "compilerOptions": {
        "assets": [
          "libs/proto/src/definitions/location.proto"
        ],
        "tsConfigPath": "apps/location/tsconfig.app.json"
      }
    },
    "dbchange": {
      "type": "application",
      "root": "apps/dbchange",
      "entryFile": "main",
      "sourceRoot": "apps/dbchange/src",
      "compilerOptions": {
        "tsConfigPath": "apps/dbchange/tsconfig.app.json"
      }
    },
    "websocket-gateway": {
      "type": "application",
      "root": "apps/websocket-gateway",
      "entryFile": "src/main",
      "sourceRoot": "./",
      "compilerOptions": {
        "assets": [
          "libs/proto/src/definitions/location.proto"
        ],
        "tsConfigPath": "apps/websocket-gateway/tsconfig.app.json"
      }
    },
    "apitypes": {
      "type": "library",
      "root": "libs/apitypes",
      "entryFile": "index",
      "sourceRoot": "libs/apitypes/src",
      "compilerOptions": {
        "tsConfigPath": "libs/apitypes/tsconfig.lib.json"
      }
    },
    "vendor": {
      "type": "application",
      "root": "apps/location",
      "entryFile": "main",
      "sourceRoot": "apps/vendor/src",
      "compilerOptions": {
        "assets": [
          "libs/proto/src/definitions/vendor.proto"
        ],
        "tsConfigPath": "apps/vendor/tsconfig.app.json"
      }
    }
  }
}