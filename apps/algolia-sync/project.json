{
	"name": "algolia-sync",
	"$schema": "../../node_modules/nx/schemas/project-schema.json",
	"sourceRoot": "apps/algolia-sync/src",
	"projectType": "application",
	"targets": {
		"build": {
			"executor": "@nx/webpack:webpack",
			"outputs": ["{options.outputPath}"],
			"defaultConfiguration": "production",
			"options": {
				"target": "node",
				"compiler": "tsc",
				"outputPath": "dist/apps/algolia-sync",
				"main": "apps/algolia-sync/src/main.ts",
				"tsConfig": "apps/algolia-sync/tsconfig.app.json",
				"assets": [],
				"isolatedConfig": true,
				"webpackConfig": "apps/algolia-sync/webpack.config.js"
			},
			"configurations": {
				"development": {
					"optimization": false,
					"extractLicenses": false,
					"inspect": false
				},
				"production": {
					"optimization": true,
					"extractLicenses": true,
					"inspect": false
				}
			}
		},
		"serve": {
			"executor": "@nx/js:node",
			"defaultConfiguration": "development",
			"options": {
				"buildTarget": "algolia-sync:build"
			},
			"configurations": {
				"development": {
					"buildTarget": "algolia-sync:build:development"
				},
				"production": {
					"buildTarget": "algolia-sync:build:production"
				}
			}
		},
		"test": {
			"executor": "@nx/vite:test",
			"outputs": ["{options.reportsDirectory}"],
			"options": {
				"passWithNoTests": true,
				"reportsDirectory": "../../coverage/apps/algolia-sync"
			}
		},
		"lint": {
			"executor": "@nx/eslint:lint",
			"outputs": ["{options.outputFile}"]
		}
	},
	"tags": ["scope:backend", "type:app"],
	"implicitDependencies": ["nest"]
}
